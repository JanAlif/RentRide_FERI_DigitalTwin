CarRide "City Tour" {
    let p = (2, 2);
    carpx = 2;
    carpy = 3;

    //let carPosition = (2, 3);

    road {
        bend ((1, 1), (2, 2), 20);
        bend ((2, 2), (3, 1), 40);
        line ((2, 2), (2, 5));
        bend ((2, 3), p, 20);
        line (p, (6, 4));
    };

    car "name" {
        carPoint (carpx, carpy)
        //carPoint carPosition;
        id "1234";
    };

    start {(1, 1)};
    end {(2, 4)};

    crossSection "rand intersection" {
        box ((3, 2), (5, 3));
    };
    crossSection "Main Intersection" {
        box ((3, 2), (5, 3));
    };

    roundAbout "Central Roundabout" {
        circ ((9, 4), 0.4);
    };

    gasStation "Central Charging Station" {
        (7, 3);
        (7, 3);
        (7, 3);
        (7, 3);
        (7, 3);
        (7, 3);

        let roi = neigh ((3, 4), 3);
            foreach x in roi {
            highlight x
        }
    };

    electricStation "Central Gas Station" {
        (7, 3);
        (7, 3);
        (7, 3);
        (7, 3);
        (7, 3);
        (7, 3);

        let roi = neigh ((3, 4), 3);
            foreach x in roi {
            highlight x
        }
    };

    parking "Nearby Parking" {
        (7, 3);
        (7, 3);
        (7, 3);
        (7, 3);
        (7, 3);
        (7, 3);

        let roi = neigh ((3, 4), 3);
            foreach x in roi {
            highlight x
        }
    }

    passenger "Visitor" {
        start {(1, 1)};

        end {(2, 4)};
    }
}